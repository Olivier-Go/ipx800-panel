{
  "filename": "manifest.webmanifest",
  "name": "IPX800 Panel",
  "short_name": "IPX800 Panel",
  "description": "PWA for GCE Electronics IPX800 controls",
  "background_color": "#4527A0",
  "inject": true,
  "fingerprints": true,
  "theme_color": "#4527A0",
  "display": "standalone",
  "start_url": ".",
  "includeDirectory": true,
  "icons": [
    {
      "src": "src/assets/favicon.png",
      "sizes": [96, 128, 192, 256, 384, 512]
    },
    {
      "src": "src/assets/favicon.png",
      "size": "1024x1024"
    },
    {
      "src": "src/assets/favicon.png",
      "size": "1024x1024",
      "purpose": "maskable"
    }
  ],
  "ios": {
    "apple-mobile-web-app-title": "IPX800 Panel",
    "apple-mobile-web-app-status-bar-style": "#4527A0"
  }
}
